{
   "name": "instagram-graph-api-sdk",
   "version": "1.0.0",
   "authoe" : "Talha Ansari",
   "description": "Type-safe TypeScript SDK for Instagram Graph API with Instagram Login",
   "main": "dist/index.js",
   "module": "dist/index.mjs",
   "types": "dist/index.d.ts",
   "exports": {
      ".": {
         "types": "./dist/index.d.ts",
         "import": "./dist/index.mjs",
         "require": "./dist/index.js"
      }
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/your-username/instagram-graph-api-sdk"
   },
   "files": [
      "dist"
   ],
   "scripts": {
      "build": "tsup",
      "dev": "tsup --watch",
      "test": "vitest",
      "test:coverage": "vitest --coverage",
      "lint": "eslint src --ext .ts",
      "docs:generate": "typedoc",
      "prepublishOnly": "npm run build"
   },
   "keywords": [
      "instagram",
      "instagram-api",
      "instagram-graph-api",
      "meta",
      "social-media",
      "sdk",
      "typescript"
   ],
   "author": "",
   "license": "MIT",

   "dependencies": {
      "axios": "^1.6.7"
   },
   "devDependencies": {
      "@types/node": "^20.11.16",
      "dotenv": "^17.2.3",
      "tsup": "^8.0.1",
      "tsx": "^4.21.0",
      "typedoc": "^0.28.16",
      "typedoc-plugin-markdown": "^4.9.0",
      "typescript": "^5.3.3",
      "vitest": "^1.2.2"
   },
   "engines": {
      "node": ">=18.0.0"
   }
}